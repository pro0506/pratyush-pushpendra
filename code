class Student
  {
    string name;
    int id;
    double percentage;
    list<string>  skills;
//single vars
     Student(String name)
      {this.name=name;}
     Student(int id)
      {this.id=id;}
    Student(double percentage)
      {this.percentage=percentage;}
    Student(list<string> skills)
      {this.skills=skills;}
//double vars
    Student(String name, int id)
      {this.name=name;
      this.id=id;}

    Student(String name, double percentage )
      {this.name=name;
      this.percentage=percentage;}

    Student(String name, list<string> skills)
      {this.name=name;
      this.skills=skills;}

    Student(String name, int id)
      {this.name=name;
      this.id=id;}
    Student(douuble percentage, int id)
      {this.percentage=percentage;
      this.id=id;}
    Student(list<string> skills, int id)
      {this.skills=skills;
      this.id=id;}
    Student(list<string> skills, double percentage)
      {this.skills=skills;
      this.percentage=percentage;}
//triple vars
    Student(String name, int id, double percentage)
      {this.name=name;
      this.id=id;
      this.percentage=percentage;}
    Student(String name, int id, list<string> skills)
      {this.name=name;
      this.id=id;
      this.skills=skills;}
    Student(String name, double percentage, list<string> skills)
      {this.name=name;
      this.percentage=percentage;
      this.skills=skills;
}
    Student( int id, double percentage, list<string> skills)
      {this.id=id;
      this.percentage=percentage;
      this.skills=skills;}
//quad var
    Student(String name, int id, double percentage, list<string> skills)
      { this.name=name;
      this.id=id;
      this.percentage=percentage;
      this.id=skills;}

//getter
    public int getname() 
      {return name;}
    public int getid() 
      {return id;}
    public int getpercentage() 
      {return percentage;}
    public list<string> getskills() 
      {return skills;}



}

    class operations extends student {
    public void changeName(String name){
        this.name = name;
    }
    public void changeName(){
        System.out.prinltn("Enter the new name");
        String name = sc.next();
        this.name = name;
    }
    public void addSkill(){
        int sk = 6;
        while(sk>5){
            System.out.println("Enter the no of skills you want to add. It shoould be max 5");
            sk = sc.nextInt();
        }
        for(int i = 0;i<sk;i++){
            System.out.println("Enter the skill");
            String skill = sc.next();
            this.skills.add(skill);
      }}}
class displayOperations extends operations{
    public void showAttributes(){
        System.out.prinltn(this.id);
        System.out.prinltn(this.name);
        System.out.prinltn(this.percentage);
        System.out.prinltn(this.skills);
    }
    public void showName(){
        System.out.prinltn(this.name.toUpperCase());
    }
    public void showDifference(displayOperations d){
        System.out.prinltn(this.percentage-d.percentage);
    }
    }
}


